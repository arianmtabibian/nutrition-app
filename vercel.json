{
  "version": 2,
  "buildCommand": "cd client && npm install && npm run build",
  "outputDirectory": "client/build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "https://nutrition-back-jtf3.onrender.com"
  },
  "build": {
    "env": {
      "REACT_APP_API_URL": "https://nutrition-back-jtf3.onrender.com"
    }
  },
  "github": {
    "autoAlias": false,
    "autoJobCancelation": false
  },
  "cleanUrls": true,
  "trailingSlash": false
}